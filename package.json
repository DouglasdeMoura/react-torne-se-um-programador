{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "storybook": "start-storybook -p 6006 --output-dir dist/storybook",
    "build-storybook": "build-storybook",
    "prepare": "husky install",
    "prisma:migrate": "dotenv -e .env.local -- npx prisma migrate dev",
    "prisma:generate": "dotenv -e .env.local -- npx prisma generate",
    "prisma:studio": "dotenv -e .env.local -- npx prisma studio"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "^4.3.1",
    "@react-hook/merged-ref": "^1.3.2",
    "@tanstack/react-query": "^4.3.4",
    "axios": "^0.27.2",
    "dotenv-cli": "^6.0.0",
    "iconoir-react": "^5.3.0",
    "next": "^12.3.0",
    "next-auth": "^4.10.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@next/eslint-plugin-next": "^12.3.1",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
